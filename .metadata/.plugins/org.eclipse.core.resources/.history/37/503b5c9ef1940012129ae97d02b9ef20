package databaseOps;

import java.sql.ResultSet;
import java.util.Collection;

import models.IDbModel;


public class QueryResult<T extends IDbModel> {
	public String sql = "";
	public Collection<T> queryResults;
	public ResultSet autoGeneratedKeys;
	public String resultStr;
	
	public T firstOrDefault(){
		try{
			return queryResults.iterator().next();
		}
		catch(Exception e){
			return null;
		}
	}
	
	public String getAutoGeneratedKey(){
		return Integer.toString(autoGeneratedKeys.getInt(1));
	}
}
