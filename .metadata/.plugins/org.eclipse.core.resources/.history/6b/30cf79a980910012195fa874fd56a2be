package databaseOps;

import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

import models.IDbModel;

public class Select extends DatabaseOp{

	private Select(IDbModel table)
			throws IllegalArgumentException, IllegalAccessException {
		super(table);
	}
	
	public static Select from(IDbModel table) throws IllegalArgumentException, IllegalAccessException{
		return new Select(table);
	}

	public ResultSet execute(Statement stmt) throws SQLException {
		sql = "select from "+tableName+" ";
		return null;
	}

}
