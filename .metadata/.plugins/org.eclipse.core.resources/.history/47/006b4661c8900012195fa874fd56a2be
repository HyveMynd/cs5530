package models;

public class Trusted {
	private int cid1;
	private int cid2;
	private boolean isTrusted;
	
	public int getCid1() {
		return cid1;
	}
	public void setCid1(int cid1) {
		this.cid1 = cid1;
	}
	public int getCid2() {
		return cid2;
	}
	public void setCid2(int cid2) {
		this.cid2 = cid2;
	}
	public boolean isTrusted() {
		return isTrusted;
	}
	public void setTrusted(boolean isTrusted) {
		this.isTrusted = isTrusted;
	}
}
