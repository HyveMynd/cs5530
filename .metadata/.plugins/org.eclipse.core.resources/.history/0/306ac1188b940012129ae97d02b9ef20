package cs5530;

import java.util.Collection;

import models.IDbModel;


public class Helper {
	public static <T extends IDbModel> String tablesToString(Collection<T>... allTables){
		String results = "";
		for (Collection<T> tables : allTables)
			for (T table : tables){\
				results += table.toString();
		return results;
	}
}
