package databaseOps;

import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

import models.IDbModel;

public class Update extends DatabaseOp{

	private Update(Statement sql, IDbModel table)
			throws IllegalArgumentException, IllegalAccessException {
		super(sql, table);
		// TODO Auto-generated constructor stub
	}

	public static ResultSet set(Statement sql, IDbModel table) throws IllegalArgumentException, IllegalAccessException, SQLException{
		Update op = new Update(sql, table);
		return op.execute();
	}
}
