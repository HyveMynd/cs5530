package databaseOps;

import java.sql.ResultSet;
import java.sql.SQLException;
import java.util.Collection;

import models.IDbModel;


public class QueryResult<T extends IDbModel> {
	public String sql = "";
	public Collection<T> queryResults;
	private int autoGeneratedKey;
	public String resultStr;
	
	public T firstOrDefault(){
		try{
			return queryResults.iterator().next();
		}
		catch(Exception e){
			return null;
		}
	}
	
	public void setKey (ResultSet keyset){
		try {
			autoGeneratedKey = keyset.getInt(1);
		} catch (SQLException e) {
			e.printStackTrace();
		}
	}
}
